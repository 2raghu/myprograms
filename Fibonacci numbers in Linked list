/*Program to store 7 fibonacci numbers in single linked list*/
#include  <stdio.h>
#include <string.h>

struct fibl {
    int data;
    struct fibl *next;
};
static int prev = 0;
struct fibl *head = NULL;
int n=0;

void fiblist(struct fibl *p,int val)
{
    struct fibl *tmp;
    
    if(p == NULL) {
        p = (struct fibl*)malloc(sizeof(struct fibl));
        p->data = val;
        p->next = NULL;
        prev = val;
        head = p;
    }
    if(n > 7)
        return;
    tmp = (struct fibl*)malloc(sizeof(struct fibl));
    tmp->next = NULL;
    if(val == 0)
        val = 1;
    else {
        val = prev+val;
        prev = p->data;
    }
    tmp->data = val;
    p->next = tmp;
    p = tmp;
    n++;
    fiblist(p,val);
    return;
}

int main(void)
{
    struct fibl *t;
    
    fiblist(head, 0);
    
    for(t = head;t!=NULL;t=t->next)
        printf("%d, ",t->data);
    return 0;
}
